public class Bit_manipulation_swap_no_2 {

    // Time Complexity: O(1)
    // Space Complexity: O(1) (only the array holds 2 ints)
    public static void swap(int[] arr){
        arr[0] = arr[0] ^ arr[1];
        arr[1] = arr[0] ^ arr[1];
        arr[0] = arr[0] ^ arr[1];
    }

    public static void main(String[] args) {
        int[] numbers = {5, 10};
        swap(numbers);
        System.out.println(numbers[0] + " " + numbers[1]); // Output: 10 5
    }
}
